{
  "expo": {
    "name": "모두의 추천목록",
    "slug": "similarmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "splash": {
      "image": "./src/assets/splash_image.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gigichoi.similarmate",
      "googleServicesFile": "./google-services.json",
      "icon": "./src/assets/appLogo.png",
      "versionCode": 2,
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./src/assets/appLogo.png"
      },
      "permissions": []
    },
    "scheme": "com.gigichoi.similarmate",
    "web": {
      "favicon": ""
    },
    "plugins": ["@react-native-google-signin/google-signin"]
  }
}
